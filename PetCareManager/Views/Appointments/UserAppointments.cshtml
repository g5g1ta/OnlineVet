@model IEnumerable<PetCareManager.Models.Appointment>

<h2 style="text-align:center;">Available Vets</h2>
<table class="table mt-3">
    <thead>
        <tr>
            <th>Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var appointment in Model)
        {
            <tr>
                <th>@appointment.AppointmentDate</th>
                <th>@appointment.Status</th>
            </tr>
        }
    </tbody>
</table>